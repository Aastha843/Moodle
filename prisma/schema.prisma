generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Snippet {
  id        String   @id @default(cuid())
  title     String
  html      String   // store your generated HTML+JS+inline CSS
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
